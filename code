int led1 = D7; 
int photoresistor = A0;
int digitalValue;





void setup() {

  
  Particle.variable("Sunlight is there", &digitalValue, INT);
  Particle.variable("Sunlight is there", &photoresistor, INT);
  pinMode(led1, OUTPUT);
  
  pinMode(photoresistor,INPUT);

}


void loop() {
 
 if {
     digitalWrite(led1, HIGH);
     
 }
 else{
     digitalWrite(led1,LOW);
     
 }

}
